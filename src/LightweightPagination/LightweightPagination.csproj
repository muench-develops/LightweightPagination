    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>muench-develops.LightweightPagination</PackageId>
    <Version>1.0.0</Version>
    <Authors>Justin Muench</Authors>
